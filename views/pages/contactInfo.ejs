<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Foodieta</title>
    <!-- favicon -->
    <link rel="icon" href="logoused.ico" type="image/x-icon">
    <!-- style -->
    <link rel="stylesheet" href="./login.css">

    <link rel="stylesheet" href="styles/reset.css">
    <link rel="stylesheet" href="styles/home.css">
    <!-- font awesome -->
    <script src="https://kit.fontawesome.com/a24ea374e8.js" crossorigin="anonymous"></script>
    <!-- google font -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/contactinfo.css">
</head>
<body>
  <header class="top-navbar">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container">
            <a class="navbar-brand" href="/">
                <img src="images/logo.png" alt="" />
            </a>
            <div class="collapse">
                <ul class="navbar-nav">
                    <li class="nav-item active"><a class="nav-link" href="home.html">Home</a></li>
                    <li class="nav-item"><a class="nav-link" href="/searches">Search</a></li>
                    <!-- <li class="nav-item"><a class="nav-link" href="">Recipe</a></li> -->
                    <li class="nav-item">
                        <a class="nav-link" href="/movieta" data-toggle="dropdown">Movieta</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/aboutus.html" id="dropdown-a">About</a>
                    </li>
                    <li class="nav-item"><a class="nav-link" href="/contact-us.html">Contact</a></li>
                </ul>
            </div>
        </div>
    </nav>
</header>
    <div id="Page">
        <% searchArray.forEach(element=>{ %>
            <div class="card-container">
        <div class="card u-clearfix">
          <div class="card-body">
            <h2 class="card-title"> <%= element.firstname%> <%= element.lastname%> </h2>
            <span class="card-description subtle"> <p class = "sumDes">email: </p> <%= element.email %></span>
            <span class="card-description subtle"> <p class = "sumDes">comments: </p> <%= element.comments %></span>
          </div>
        </div>
      </div>
      <%}) %>
</div> 
</body>
</html>